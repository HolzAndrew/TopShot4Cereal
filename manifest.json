{
"manifest_version": 2,
"name": "Topshot 4 Cereal",
"description": "Chrome Extention that shows Dollars Per Cereal of a Topshop Moment",
"version": "1.0",
"icons": {
  "16": "logo_16.jpeg",
  "48": "logo_48.jpeg",
  "128": "logo_128.jpeg"
},
"content_scripts":[
    {
      "matches":["https://nbatopshot.com/listings/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
"browser_action": {
    "default_icon": "logo.jpeg"
  },
"background": {
      "scripts": ["background.js"],
      "persistent": false
    },
"permissions": [
    "tabs", 
    "https://nbatopshot.com/listings/*"
  ]
}